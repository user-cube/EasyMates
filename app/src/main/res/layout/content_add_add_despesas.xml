<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Add_AddDespesas">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="417dp"
        android:layout_height="160dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/houseName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/imagem_aveiro" />

    <TextView
        android:id="@+id/element9"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/my_border"
        android:contentDescription="element4"
        android:gravity="center"
        android:onClick="editDespesa4"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/element3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/element" />

    <TextView
        android:id="@+id/element3"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/my_border"
        android:contentDescription="element5"
        android:gravity="center"
        android:onClick="editDespesa5"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/floatingActionButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/element9" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:onClick="addDespesa"
        app:backgroundTint="#00BCD4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/element3"
        app:srcCompat="@drawable/ic_add_white_24dp" />

    <TextView
        android:id="@+id/houseName"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:gravity="center"
        android:text="Recentes"
        android:textColor="#AB323232"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/elementOne"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView15" />

    <TextView
        android:id="@+id/elementOne"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/my_border"
        android:contentDescription="element1"
        android:gravity="center"
        android:onClick="editDespesa"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/elementTwo2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/houseName" />

    <TextView
        android:id="@+id/element"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/my_border"
        android:contentDescription="element3"
        android:gravity="center"
        android:onClick="editDespesa3"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/element9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elementTwo2" />

    <TextView
        android:id="@+id/elementTwo2"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/my_border"
        android:contentDescription="element2"
        android:gravity="center"
        android:onClick="editDespesa2"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/element"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elementOne" />

</android.support.constraint.ConstraintLayout>